{
  "name": "viettrace-core",
  "version": "0.1.3",
  "description": "Core logic for viettrace projects",
  "author": "Lam Tran <lam.tttech19@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "open-source",
    "vietnam"
  ],
  "engines": {
    "pnpm": ">=9.0.0",
    "node": "22.16.0"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "prepare": "lefthook install || true",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "release-it",
    "release:dry": "release-it --dry-run",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "knip": "knip --no-exit-code",
    "knip:fix": "knip --fix",
    "knip:production": "knip --production --no-exit-code",
    "studio": "drizzle-kit studio",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "11.2.0",
    "@sentry/nestjs": "9.40.0",
    "@sentry/profiling-node": "9.40.0",
    "axios": "1.11.0",
    "chalk": "5.4.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "compression": "1.8.0",
    "drizzle-orm": "0.44.2",
    "helmet": "8.1.0",
    "nestjs-cls": "6.0.1",
    "pg": "8.16.3",
    "reflect-metadata": "^0.2.2",
    "request-ip": "3.3.0",
    "rxjs": "^7.8.1",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "zod": "4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.31.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/compression": "1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "8.15.4",
    "@types/request-ip": "0.0.41",
    "@types/supertest": "^6.0.2",
    "cross-env": "7.0.3",
    "drizzle-kit": "0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^30.0.4",
    "knip": "^5.61.3",
    "lefthook": "^1.11.16",
    "lint-staged": "^16.1.2",
    "prettier": "^3.4.2",
    "release-it": "^19.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.36.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
